{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ORIGEM",
						"type": "DatasetReference"
					},
					"name": "pesquisatrabalhofuturo"
				},
				{
					"dataset": {
						"referenceName": "ORIGEM",
						"type": "DatasetReference"
					},
					"name": "trabalhofuturocaracteristicas"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ORIGEM",
						"type": "DatasetReference"
					},
					"name": "DESTINO"
				}
			],
			"transformations": [
				{
					"name": "union1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Id_Pesquisa as short,",
				"          Data_Pesquisa as string,",
				"          Satisfacao_Atual as short,",
				"          Equilibrio_Vida_Trabalho as short,",
				"          Saude_Mental_Status as short",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 100,",
				"     ignoreNoFilesFound: false) ~> pesquisatrabalhofuturo",
				"source(output(",
				"          Id_Pesquisa as short,",
				"          Estado as string,",
				"          Cidade as string,",
				"          Genero as string,",
				"          Idade as short,",
				"          Profissao as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> trabalhofuturocaracteristicas",
				"pesquisatrabalhofuturo, trabalhofuturocaracteristicas union(byName: true)~> union1",
				"union1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> DESTINO"
			]
		}
	}
}